<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_nav_drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mx.com.sybrem.appbiochem.NavDrawerActivity"
    tools:showIn="@layout/app_bar_nav_drawer">

    <com.github.snowdream.android.widget.SmartImageView
        android:id="@+id/img_inicio"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:scaleType="fitCenter"
        android:src="@drawable/menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="0.9"
        android:layout_marginTop="85dp" />

